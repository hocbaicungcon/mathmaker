<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:include schemaLayout="nb_source.xsd"/>
	<xsd:include schemaLayout="question.xsd"/>

	<xsd:element name="mix">
		<xsd:complexType>
			<xsd:all>
				<xsd:element ref="question" 	minOccurs="1"	maxOccurs="unbounded"/>
				<xsd:element ref="nb_source"	minOccurs="1"	maxOccurs="unbounded"/>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>

</xsd:schema>
